{
    "name": "calima-solutions/calima-genesis",
    "description": "CLI tool for Calima developers.",
    "keywords": ["calima", "cli", "starter kit", "helper"],
    "homepage": "https://genesis.by.calimasolutions.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/calima-solutions/Calima-Genesis/issues",
        "source": "https://github.com/calima-solutions/Calima-Genesis"
    },
    "authors": [
        {
            "name": "Raúl López",
            "email": "raul@calimasolutions.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/http": "^9.0",
        "laminas/laminas-text": "^2.9",
        "laravel-zero/phar-updater": "^1.2",
        "nunomaduro/termwind": "^1.13"
    },
    "require-dev": {
        "laravel-zero/framework": "^9.1.3",
        "laravel/pint": "^1.1",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/genesis"]
}
